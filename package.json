{
  "name": "webdeveassisment-01",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "set NODE_ENV=test && mocha test/cargo-test.js",
    "publish-coverage": "nyc report --reporter=text-lcov | coveralls",
    "coverage": "cross-env NODE_ENV=test PORT=3000 nyc mocha test/cargo-test.js",
    "clean": "rimraf ./lib && mkdir lib",
    "server:watch": "cross-env NODE_ENV=dev nodemon --exec babel-node bin/www",
    "test:watch": "npm run test -- --watch",
    "lint": "esw  ./test ./routes ./models/ app.js",
    "lint:watch": "npm run lint -- --watch"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "lodash": "^4.17.4",
    "mongo": "^0.1.0",
    "mongoose": "^5.3.2",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.5",
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "chai-things": "^0.2.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.2.0",
    "eslint-watch": "^4.0.2",
    "mocha": "^3.4.2",
    "nodemon": "^1.18.7",
    "nyc": "^13.1.0"
  }
}
